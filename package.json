{
  "name": "apollo-mysql-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-client": "^0.10.1",
    "apollo-server": "^0.3.3",
    "body-parser": "^1.17.0",
    "express": "^4.15.0",
    "graphql": "^0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-tag": "^1.3.1",
    "graphql-tools": "^0.10.1",
    "graphql-typings": "0.0.1-beta-2",
    "mysql": "^2.13.0",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-apollo": "^0.13.2",
    "react-dom": "^15.4.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-form": "^6.3.2",
    "sequelize": "^3.30.2"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.3",
    "@types/sinon": "^1.16.34",
    "babel-cli": "^6.23.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "concurrently": "3.1.0",
    "cors": "^2.8.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "graphql": "^0.9.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm-check-updates": "^2.8.9",
    "react-scripts": "0.9.3",
    "remap-istanbul": "^0.9.1",
    "sinon": "^1.17.7",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2",
    "tslint": "^4.3.1",
    "typescript": "^2.1.5"
  },
  "scripts": {
    "lint": "tslint ./packages/**/src/**/*.ts --exclude \"./packages/**/node_modules/**/*.ts\"",
    "prebootstrap": "npm install",
    "pretest": "npm run compile",
    "test": "react-scripts test --env=jsdom",
    "posttest": "npm run lint",
    "testonly": "mocha --reporter spec --full-trace --timeout 5000 ./test/tests.js",
    "coverage": "istanbul cover -x \"*.test.js\" _mocha -- --timeout 5000 --full-trace --reporter dot ./test/tests.js",
    "pretravis": "npm run compile",
    "travis": "istanbul cover -x \"*.test.js\" _mocha -- --timeout 5000 --full-trace ./test/tests.js",
    "posttravis": "npm run lint",
    "postcoverage": "remap-istanbul --input coverage/coverage.raw.json --type lcovonly --output coverage/lcov.info",
    "start": "react-scripts start",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "server": "nodemon server.js --exec babel-node",
    "client": "babel-node start-client.js",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  }
}
